<div class="wrapper wrapper-content">
  <div class="ibox float-e-margins">
    <div class="ibox-content">
      <%= form_for(case_team) do |f| %>
        <% if case_team.errors.any? %>
          <div class="panel panel-danger">
            <div class="panel-heading">
              <%= pluralize(case_team.errors.count, "error") %> prohibited this case_team from being saved.
            </div>

            <div class="panel-body">
              <ul>
                <% case_team.errors.full_messages.each do |message| %>
                <li><%= message %></li>
                <% end %>
              </ul>
            </div>
          </div>
        <% end %>

                <div class="form-group">
                            <%= f.label :name %>
                <%= f.text_field :name, :class => 'form-control' %>
                        </div>

                <div class="form-group">
                            <%= f.label :discription %>
                <%= f.text_field :discription, :class => 'form-control' %>
                        </div>

                <div class="form-group">
                            <%= f.label :role_type %>
                <%= f.text_field :role_type, :class => 'form-control' %>
                        </div>

      
      <div class="actions">
        <%= f.submit :class => 'btn btn-primary' %>
      </div>
  <% end %>

    </div>
  </div>
</div>